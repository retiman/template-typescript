{
  "name": "Typescript",
  "build": {
    "dockerfile": "Dockerfile",
    "args": {
      "VARIANT": "18-buster"
    }
  },
  "settings": {},
  "extensions": [
    "EditorConfig.EditorConfig",
    "Orta.vscode-jest",
    "Tyriar.windows-terminal",
    "medo64.render-crlf",
    "ms-vscode.vscode-typescript-next"
  ],
  "mounts": ["source=node_modules,target=${containerWorkspaceFolder}/node_modules,type=volume"],
  "remoteEnv": {
    "PATH": "${containerEnv:PATH}:${containerWorkspaceFolder}/node_modules/.bin"
  },
  "remoteUser": "node",
  "features": {
    "git": "2.20.1"
  }
}
